{% trans_default_domain 'notifications' %}

{% if menu %}
    {% block menu_preview %}
        {{ notification_menu_preview(notification, title, content)|raw }}
    {% endblock %}
{% elseif preview %}
    {% block preview %}
        {{ notification_preview(notification, title, content)|raw }}
    {% endblock %}
{% else %}
    {% block complete %}
        {{ notification_complete(notification, title, content)|raw }}
    {% endblock %}
{% endif %}

